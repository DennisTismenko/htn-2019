{
  "name": "@nodome/www",
  "version": "1.0.0",
  "description": "nodome.io WWW client",
  "main": "src/index.js",
  "scripts": {
    "start": "node -r dotenv/config .",
    "start:dev": "parcel serve --host=$HOST --port=$PORT --hmr-port=$HMR_PORT src/client/index.html",
    "build": "./scripts/build.sh && npm pack",
    "build:dev": "nodemon -w src -e ts,tsx,js,jsx -x 'yarn build && yarn start'",
    "clean": "rm -rf dist",
    "lint": "yarn prettier && yarn eslint",
    "lint:fix": "yarn prettier:fix && yarn eslint:fix",
    "prettier": "yarn prettier:cli -l",
    "prettier:cli": "prettier --parser typescript '**/*.ts' '**/*.tsx' '**/*.js' '**/*.jsx'",
    "prettier:fix": "yarn prettier:cli --write",
    "eslint": "eslint . --ext tsx,ts,jsx,js",
    "eslint:fix": "yarn eslint --fix"
  },
  "author": "Omar Chehab <omarchehab98@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "disqus-react": "^1.0.7",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "styled-components": "^4.3.2",
    "superagent": "^5.1.0"
  },
  "bundledDependencies": [
    "express",
    "lodash",
    "react",
    "react-dom",
    "react-router",
    "react-router-dom",
    "styled-components"
  ],
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@svgr/parcel-plugin-svgr": "^4.3.2",
    "@types/express": "^4.17.1",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.10",
    "@types/react-router-dom": "^4.3.5",
    "@types/styled-components": "^4.1.19",
    "@types/superagent": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "dotenv": "^8.1.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "nodemon": "^1.19.2",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "typescript": "^3.6.3"
  }
}
